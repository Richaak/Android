<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#e6e7fe"
    android:layout_height="match_parent"
    tools:context=".page1">


    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appBarLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ff2701"
        tools:ignore="MissingConstraints">

        <com.google.android.material.appbar.MaterialToolbar
            android:id="@+id/tool"
            android:layout_width="match_parent"
            android:layout_height="match_parent">


        </com.google.android.material.appbar.MaterialToolbar>


    </com.google.android.material.appbar.AppBarLayout>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="351dp"
        android:layout_height="233dp"
        android:layout_marginStart="30dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"

        android:layout_marginBottom="29dp"
        app:layout_constraintBottom_toTopOf="@+id/textInputLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/appBarLayout"
        tools:ignore="MissingConstraints,NotSibling,UnknownId">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/a"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="29dp"></ImageView>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="383dp"
        android:layout_height="369dp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:foregroundGravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints">



        <LinearLayout
            android:layout_width="353dp"
            android:layout_height="386dp"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:background="#afc4b5"

            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/appBarLayout"
            app:layout_constraintVertical_bias="0.906"
            tools:ignore="MissingConstraints">


            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/textInputLayout"
                android:layout_width="342dp"
                android:layout_height="114dp"
                android:hint="Name"
                app:boxBackgroundColor="#ffffaa"
                app:boxBackgroundMode="outline"
                app:boxCornerRadiusBottomEnd="26dp"
                app:boxCornerRadiusTopStart="26dp"
                app:helperText="Enter Your First Name Here"
                app:startIconDrawable="@drawable/name"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="37dp"
                tools:layout_editor_absoluteY="337dp">

                <com.google.android.material.textfield.TextInputEditText
                    android:layout_width="match_parent"
                    android:layout_height="86dp"
                    android:layout_marginLeft="10dp">


                </com.google.android.material.textfield.TextInputEditText>


            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/textInputLayout2"
                android:layout_width="342dp"
                android:layout_height="114dp"
                android:hint="Number"
                app:boxBackgroundColor="#ffffaa"
                app:boxBackgroundMode="outline"
                app:boxCornerRadiusBottomEnd="26dp"
                app:boxCornerRadiusTopStart="26dp"
                app:helperText="Enter Your Mobile Number Here"
                app:startIconDrawable="@drawable/phone"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="37dp"
                tools:layout_editor_absoluteY="337dp">

                <com.google.android.material.textfield.TextInputEditText
                    android:layout_width="match_parent"
                    android:layout_height="86dp"
                    android:layout_marginLeft="10dp">


                </com.google.android.material.textfield.TextInputEditText>


            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/textInputLayout3"
                android:layout_width="342dp"
                android:layout_height="114dp"
                android:hint="Email"
                app:boxBackgroundColor="#ffffaa"
                app:boxBackgroundMode="outline"
                app:boxCornerRadiusBottomEnd="26dp"
                app:boxCornerRadiusTopStart="26dp"
                app:helperText="Enter Your Email ID Here"
                app:startIconDrawable="@drawable/email"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="37dp"
                tools:layout_editor_absoluteY="337dp">

                <com.google.android.material.textfield.TextInputEditText
                    android:layout_width="match_parent"
                    android:layout_height="86dp"
                    android:layout_marginLeft="10dp">


                </com.google.android.material.textfield.TextInputEditText>


            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/textInputLayout4"
                android:layout_width="342dp"
                android:layout_height="114dp"
                android:hint="Enter Password"
                app:boxBackgroundColor="#ffffaa"
                app:boxBackgroundMode="outline"
                app:startIconDrawable="@drawable/password"
                app:boxCornerRadiusBottomEnd="26dp"
                app:boxCornerRadiusTopStart="26dp"
                app:endIconMode="password_toggle"
                app:helperText="Enter a unique password here"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="37dp"
                tools:layout_editor_absoluteY="337dp">

                <com.google.android.material.textfield.TextInputEditText
                    android:layout_width="match_parent"
                    android:layout_height="86dp"
                    android:layout_marginLeft="10dp">


                </com.google.android.material.textfield.TextInputEditText>


            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/textInputLayout5"
                android:layout_width="342dp"
                android:layout_height="114dp"
                android:hint="Confirm Password"
                app:boxBackgroundColor="#ffffaa"
                app:startIconDrawable="@drawable/password"
                app:boxBackgroundMode="outline"
                app:boxCornerRadiusBottomEnd="26dp"
                app:boxCornerRadiusTopStart="26dp"
                app:endIconMode="password_toggle"
                app:helperText="Re-Enter the same password here"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="37dp"
                tools:layout_editor_absoluteY="337dp">

                <com.google.android.material.textfield.TextInputEditText
                    android:layout_width="match_parent"
                    android:layout_height="86dp">


                </com.google.android.material.textfield.TextInputEditText>


            </com.google.android.material.textfield.TextInputLayout>
        </LinearLayout>

    </ScrollView>


    <View
        android:id="@+id/view"
        android:layout_width="355dp"

        android:layout_height="23dp"

        android:background="#46eb75"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.458"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="63dp"
        android:layout_height="108dp"
        android:src="@drawable/cam"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.454"
        tools:ignore="MissingConstraints">


    </ImageView>

</androidx.constraintlayout.widget.ConstraintLayout>





